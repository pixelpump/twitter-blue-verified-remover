{
  "name": "Twitter Blue Verified Replacement",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://*.twitter.com/*"]
  }],
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["https://*.twitter.com/*"]
    }
  ],
  "action": {}
}
